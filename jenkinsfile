pipeline {
    agent any 
environment {
        PATH = "$PATH:/opt/flutter/bin"
    }

    stages {
        stage('clonings') { 
            steps {
               echo "cloning"
               git branch: 'main', url: 'https://github.com/said1024/flutterDemoJenkins.git' 
            }
        }
 stage('build web') { 
            steps {
               echo "build"
               sh "flutter --version" 
               sh "cd $WORKSPACE"
               sf "flutter build web"     
                 
            
            }
        }
 
    }
}
